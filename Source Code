// FormPrice.cs

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace DatabaseSQL
{
    public partial class FormPrice : Form
    {
        private readonly Form3 _parent;
        public string pid, pprice;
        public FormPrice(Form3 parent)
        {
            InitializeComponent();
            _parent = parent;
        }

        public void UpdateInfo()
        {
            this.Text = "Update Price";
            label1.Text = "Update Price";
            ButtonSave.Text = "Update";
            productCombo.Text = " ";
            this.productCombo.Hide();
            this.label2.Hide();
            textAddress.Text = pprice;
        }

        public void InsertHelper()
        {
            this.Text = "Add Price";
            ButtonSave.Text = "Save";
            label1.Text = "Add Price";
            productCombo.Text = "Select Name";
            this.productCombo.Show();
            this.label2.Show();
        }

        public void Clear()
        {
            textAddress.Text = string.Empty;
        }

        private void FormPrice_Load(object sender, EventArgs e)
        {
            // TODO: This line of code loads data into the 'jmdistDataSet1.product_t' table. You can move, or remove it, as needed.
            this.product_tTableAdapter.Fill(this.jmdistDataSet1.product_t);

        }

        private void ButtonSave_Click(object sender, EventArgs e)
        {
            if (productCombo.Text == "Select Name")
            {
                MessageBox.Show("Product Name is Not Selected");
                return;
            }
            if (textAddress.Text.Trim().Length == 0)
            {
                MessageBox.Show("Price is Empty");
                return;
            }
            if (ButtonSave.Text == "Save")
            {
                Price pri = new Price(int.Parse(_parent.id), int.Parse(productCombo.SelectedValue.ToString()), double.Parse(textAddress.Text.Trim()));
                DbPrice.AddPrice(pri);
                Clear();
            }
            if (ButtonSave.Text == "Update")
            {
                Price pri = new Price(int.Parse(_parent.id), int.Parse(pid), double.Parse(textAddress.Text.Trim()));
                DbPrice.UpdatePrice(pri);
            }

            _parent.Display();
            this.Close();
        }
    }
}




