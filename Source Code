// FormCustomer.cs

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace DatabaseSQL
{
    public partial class FormCustomer : Form
    {
        private readonly CustomerTable _parent;
       // public int id, zipCode;
        public string id, name, address, city, state, zipCode, phoneNumber;

        private void FormCustomer_Load(object sender, EventArgs e)
        {

        }

        public FormCustomer(CustomerTable parent)
        {
            InitializeComponent();
            _parent = parent;
        }

        public void UpdateInfo()
        {
            this.Text = "Update Customer";
            label1.Text = "Update Customer";
            ButtonSave.Text = "Update";
            textName.Text = name;
            textAddress.Text = address;
            textCity.Text = city;
            textState.Text = state;
            textZipCode.Text = zipCode;
            textNumber.Text = phoneNumber;
        }

        public void InsertHelper()
        {
            this.Text = "Add Customer";
            ButtonSave.Text = "Save";
            label1.Text = "Add Customer";
        }

        public void Clear()
        {
            textName.Text = textAddress.Text = textCity.Text = textState.Text = textZipCode.Text = textNumber.Text = string.Empty;
        }

        private void ButtonSave_Click(object sender, EventArgs e)
        {
            if (textName.Text.Trim().Length == 0)
            {
                MessageBox.Show("Customer Name is Empty");
                return;
            }
            if (textAddress.Text.Trim().Length == 0)
            {
                MessageBox.Show("Customer Address is Empty");
                return;
            }
            if (textCity.Text.Trim().Length == 0)
            {
                MessageBox.Show("Customer City is Empty");
                return;
            }
            if (textState.Text.Trim().Length == 0 || textState.Text.Trim().Length > 2)
            {
                MessageBox.Show("Customer State is Empty or more than two letters. \n**Please use a 2 letter input**");
                return;
            }
            if (textZipCode.Text.Trim().Length == 0)
            {
                MessageBox.Show("Customer ZipCode is Empty");
                return;
            }
            if (textNumber.Text.Trim().Length == 0)
            {
                MessageBox.Show("Customer Phone Number is Empty");
                return;
            }
            if (ButtonSave.Text == "Save")
            {
                Customer cus = new Customer(textName.Text.Trim(), textAddress.Text.Trim(), textCity.Text.Trim(), textState.Text.Trim(), int.Parse(textZipCode.Text.Trim()), textNumber.Text.Trim());
                DbCustomer.AddCustomer(cus);
                Clear();
            }
            if (ButtonSave.Text == "Update")
            {
                Customer cus = new Customer(textName.Text.Trim(), textAddress.Text.Trim(), textCity.Text.Trim(), textState.Text.Trim(), int.Parse(textZipCode.Text.Trim()), textNumber.Text.Trim());
                DbCustomer.UpdateCustomer(cus, id);
            }
            _parent.Display();
            this.Close();
        }
    }
}

