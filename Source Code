// Form3.cs

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace DatabaseSQL
{
    public partial class Form3 : Form
    {
        FormPrice frm;
        public string id, name;
        public Form3()
        {
            InitializeComponent();
            frm = new FormPrice(this);
        }
        public void Display()
        {
            DbCustomer.DisplayAndSearch("SELECT ProductID, ProductName, CustomerProductPrice FROM Customer_T JOIN CustomerProductPrice_T USING (CustomerID) JOIN Product_T USING (ProductID) WHERE CustomerID = " + id, dataGridView);
        }

        private void ButtonNew_Click(object sender, EventArgs e)
        {
            frm.Clear();
            frm.InsertHelper();
            frm.ShowDialog();
        }

        private void TextSearch_TextChanged(object sender, EventArgs e)
        {
            DbCustomer.DisplayAndSearch("SELECT ProductID, ProductName, CustomerProductPrice FROM Customer_T JOIN CustomerProductPrice_T USING (CustomerID) JOIN Product_T USING (ProductID) WHERE ProductName LIKE '%" + TextSearch.Text + "%' AND CustomerID = " + id, dataGridView);
        }

        private void dataGridView_CellClick(object sender, DataGridViewCellEventArgs e)
        {
            if (e.ColumnIndex == 0)
            {
                frm.Clear();
                frm.pid = dataGridView.Rows[e.RowIndex].Cells[2].Value.ToString();
                frm.pprice = dataGridView.Rows[e.RowIndex].Cells[4].Value.ToString();
                frm.UpdateInfo();
                frm.ShowDialog();
                return;
            }
            if (e.ColumnIndex == 1)
            {
                if (MessageBox.Show("Confirm deleting the record with the ID = " + dataGridView.Rows[e.RowIndex].Cells[2].Value.ToString() + "?", "Information", MessageBoxButtons.YesNoCancel, MessageBoxIcon.Information) == DialogResult.Yes)
                {
                    DbPrice.DeletePrice(id ,dataGridView.Rows[e.RowIndex].Cells[2].Value.ToString());
                    Display();
                }
                return;
            }
        }

        private void Form3_Shown(object sender, EventArgs e)
        {
            label1.Text = "Customer Name: " + name;
            Display();
        }

    }
}

// Price.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace DatabaseSQL
{
    class Price
    {
        public int CustomerID { get; set; }
        public int ProductID { get; set; }
        public double CusPrice { get; set; }

        public Price(int cID, int pID, double cPrice)
        {
            CustomerID = cID;
            ProductID = pID;
            CusPrice = cPrice;
        }
    }
}

//DbPrice.cs

using MySql.Data.MySqlClient;
using System;
using System.Collections.Generic;
using System.Data;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace DatabaseSQL
{
    class DbPrice
    {
        public static MySqlConnection GetConnection()
        {
            string sql = "datasource=localhost;port=3306;username=root;password=password;database=jmdist";
            MySqlConnection con = new MySqlConnection(sql);
            try
            {
                con.Open();
            }
            catch (MySqlException ex)
            {
                MessageBox.Show("MySQL Connection! \n" + ex.Message, "Error", MessageBoxButtons.OK, MessageBoxIcon.Error);
            }
            return con;
        }

        public static void AddPrice(Price pri)
        {
            string sql = "INSERT INTO CustomerProductPrice_T (CustomerID, ProductID, CustomerProductPrice) VALUES (@CustomerID, @ProductID, @CustomerProductPrice)";
            MySqlConnection con = GetConnection();
            MySqlCommand cmd = new MySqlCommand(sql, con);
            cmd.CommandType = CommandType.Text;
            cmd.Parameters.Add("@CustomerID", MySqlDbType.VarChar).Value = pri.CustomerID;
            cmd.Parameters.Add("@ProductID", MySqlDbType.VarChar).Value = pri.ProductID;
            cmd.Parameters.Add("@CustomerProductPrice", MySqlDbType.VarChar).Value = pri.CusPrice;
            try
            {
                cmd.ExecuteNonQuery();
                MessageBox.Show("Inserted Successfully.", "Information", MessageBoxButtons.OK, MessageBoxIcon.Information);
            }
            catch (MySqlException ex)
            {
                MessageBox.Show("Customer is not insterted. \n" + ex.Message, "Error", MessageBoxButtons.OK, MessageBoxIcon.Error);
            }
            con.Close();
        }
        public static void UpdatePrice(Price pri)
        {
            string sql = "UPDATE CustomerProductPrice_T SET CustomerProductPrice = @CustomerProductPrice WHERE CustomerID = @CustomerID AND ProductID = @ProductID";
            MySqlConnection con = GetConnection();
            MySqlCommand cmd = new MySqlCommand(sql, con);
            cmd.CommandType = CommandType.Text;
            cmd.Parameters.Add("@CustomerID", MySqlDbType.VarChar).Value = pri.CustomerID;
            cmd.Parameters.Add("@ProductID", MySqlDbType.VarChar).Value = pri.ProductID;
            cmd.Parameters.Add("@CustomerProductPrice", MySqlDbType.VarChar).Value = pri.CusPrice;
            try
            {
                cmd.ExecuteNonQuery();
                MessageBox.Show("Updated Successfully.", "Information", MessageBoxButtons.OK, MessageBoxIcon.Information);
            }
            catch (MySqlException ex)
            {
                MessageBox.Show("Customer is not updated. \n" + ex.Message, "Error", MessageBoxButtons.OK, MessageBoxIcon.Error);
            }
            con.Close();
        }

        public static void DeletePrice(string id, string cid)
        {
            string sql = "DELETE FROM CustomerProductPrice_T WHERE CustomerID = @CustomerID AND ProductID = @ProductID";
            MySqlConnection con = GetConnection();
            MySqlCommand cmd = new MySqlCommand(sql, con);
            cmd.CommandType = CommandType.Text;
            cmd.Parameters.Add("@CustomerID", MySqlDbType.VarChar).Value = id;
            cmd.Parameters.Add("@ProductID", MySqlDbType.VarChar).Value = cid;

            try
            {
                cmd.ExecuteNonQuery();
                MessageBox.Show("Deleted Successfully.", "Information", MessageBoxButtons.OK, MessageBoxIcon.Information);
            }
            catch (MySqlException ex)
            {
                MessageBox.Show("Customer is not deleted. \n" + ex.Message, "Error", MessageBoxButtons.OK, MessageBoxIcon.Error);
            }
            con.Close();
        }

    }
}

