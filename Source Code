// FormProduct.cs

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace DatabaseSQL
{
    public partial class FormProduct : Form
    {
        private readonly Form2 _parent;
        public string id, name;

        private void FormProduct_Load(object sender, EventArgs e)
        {

        }

        public FormProduct(Form2 parent)
        {
            InitializeComponent();
            _parent = parent;
        }

        public void UpdateInfo()
        {
            this.Text = "Update Product";
            label1.Text = "Update Product";
            ButtonSave.Text = "Update";
            textName.Text = name;
        }

        public void InsertHelper()
        {
            this.Text = "Add Product";
            ButtonSave.Text = "Save";
            label1.Text = "Add Product";
        }

        public void Clear()
        {
            textName.Text = string.Empty;
        }

        private void ButtonSave_Click_1(object sender, EventArgs e)
        {
            if (textName.Text.Trim().Length == 0)
            {
                MessageBox.Show("Product Name is Empty");
                return;
            }
            if (ButtonSave.Text == "Save")
            {
                Product pro = new Product(textName.Text.Trim());
                DbProduct.AddProduct(pro);
                Clear();
            }
            if (ButtonSave.Text == "Update")
            {
                Product pro = new Product(textName.Text.Trim());
                DbProduct.UpdateProduct(pro, id);
            }
            _parent.Display();
            this.Close();
        }
    }
}

